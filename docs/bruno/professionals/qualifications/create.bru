  name: Create Qualification
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/api/v1/professionals/:professionalId/qualifications/
  body: json
  auth: inherit
}

params:path {
  professionalId: {{professionalId}}
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "professional_type": "DOCTOR",
    "is_primary": true,
    "graduation_year": 2015,
